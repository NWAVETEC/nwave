  <xs:complexType name="DeviceStatus">
    <xs:annotation>
      <xs:documentation>Status of device</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="Resource">
        <xs:sequence>
          <xs:element name="changedTime" minOccurs="1" maxOccurs="1" type="TimeType">
